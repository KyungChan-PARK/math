*.db.8 filter=lfs diff=lfs merge=lfs -text
*.db.9 filter=lfs diff=lfs merge=lfs -text
*.transaction.db.* filter=lfs diff=lfs merge=lfs -text

# Large directories
venv311/** filter=lfs diff=lfs merge=lfs -text

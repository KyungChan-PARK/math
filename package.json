{
  "name": "ae-claude-max",
  "version": "3.3.0",
  "description": "Real-time Natural Language to After Effects Bidirectional Communication System",
  "main": "server/index.js",
  "type": "module",
  "scripts": {
    "start": "node server/index.js",
    "dev": "nodemon server/index.js",
    "test": "node tests/test-suite.js",
    "test:integration": "node tests/integration-test.js",
    "test:migration": "node tests/platform-migration-tests.cjs",
    "test:all": "npm run test && npm run test:migration && npm run test:integration",
    "test:cep": "jest --testPathPattern=cep",
    "test:uxp-mock": "jest --testPathPattern=uxp",
    "test:websocket": "jest --testPathPattern=websocket",
    "benchmark:websocket": "node benchmarks/websocket-performance.js",
    "benchmark:all": "npm run benchmark:websocket && npm run benchmark:inference",
    "migration:check": "node scripts/migration-status.js",
    "build:cep": "node scripts/build-cep-extension.js",
    "build:uxp": "node scripts/build-uxp-extension.js"
  },
  "keywords": [
    "after-effects",
    "natural-language",
    "websocket",
    "cep",
    "uxp",
    "ai",
    "motion-graphics"
  ],
  "author": "AE Claude Max Team",
  "license": "MIT",
  "dependencies": {
    "compromise": "^14.14.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "natural": "^8.1.0",
    "uuid": "^9.0.1",
    "uwebsockets.js": "^19.0.0",
    "uWebSockets.js": "github:uNetworking/uWebSockets.js#v20.44.0",
    "winston": "^3.11.0",
    "ws": "^8.16.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.5",
    "@types/ws": "^8.5.10",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "prettier": "^3.1.1",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "migration": {
    "cep_to_uxp": {
      "status": "in_progress",
      "abstraction_coverage": 0,
      "target_date": "2026-Q1"
    },
    "directml_to_windowsml": {
      "status": "pending",
      "coverage": 0,
      "target_date": "2025-10"
    },
    "ws_to_uwebsockets": {
      "status": "in_progress",
      "performance_baseline": 100,
      "performance_target": 850
    }
  }
}
